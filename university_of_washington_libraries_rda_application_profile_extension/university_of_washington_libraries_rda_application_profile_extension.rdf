  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasLcGeographicClassification">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has LC Geographic Classification</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an RDA Manifestation or BIBFRAME Instance and a Library of Congress Geographic Classification number corresponding to a geographic area represented on a cartographic resource.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Required for describing RDA Manifestations and BIBFRAME Instances of cartographic resources. No equivalent property found in another vocabulary.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasContributorAgent">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has Contributor Agent</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an RDA Work, Expression, Manifestation, or Item and an agent who has made some contribution to its realization.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Although this is a frequently used property relating RDA entities, it is not included in the RDA Resitry. The domain is unspecified, but values should be instances of one of 4 RDA entities: Work, Expression, Manifestation or Item.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasNlmClassificationPartB">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has NLM Classification Part B</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an entity and the second portion of its National Library of Medicine Classification number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Property to classify a resource. The domain and range of the BIBFRAME property created for a similar purpose was judged too restrictive.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasUniversityOfWashingtonNote">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has University Of Washington Note.</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an entity other than an RDA Item and a note.</rdfs:comment>
    <skos:note xml:lang="en">Expected to be pertinent only in one local context (the University of Washington Libraries).</skos:note>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasPhysicalLocation">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has Physical Location</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an item and a physical location for that item.</rdfs:comment>
    <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Equivalent BIBFRAME property has rdfs:domain = bf:Item, which is inaccurate in the RDA context.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#heldBy">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Held By</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an RDA Manifestation or Item and an organization that holds it.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">This property is expected to be used to describe RDA Manifestations and Items only. The domain and range of the BIBFRAME property created for this purpose was judged too restrictive.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasSuDocClassificationNumber">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has SuDoc Classification Number</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between a government document and a corresponding U.S. Superintendent of Documents Classification number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Required for describing RDA Manifestations that are government documents, RDA Items, and BIBFRAME Instances of cartographic resources. No equivalent property found in another vocabulary.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasLcClassificationPartB">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has LC Classification Part B</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an entity and the second portion of its Library of Congress Classification number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Property to classify a resource. The domain and range of the BIBFRAME property created for a similar purpose was judged too restrictive.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasNlmClassificationPartA">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has NLM Classification Part A</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an entity and the first portion of its National Library of Medicine Classification number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Property to classify a resource. The domain and range of the BIBFRAME property created for a similar purpose was judged too restrictive.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasLcClassificationPartA">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has LC Classification Part A</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an entity and the first portion of its Library of Congress Classification number.</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Property to classify a resource. The domain and range of the BIBFRAME property created for a similar purpose was judged too restrictive.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasCallNumber">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has Call Number</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an item and its call number, regardless of the use of a formal classification scheme.</rdfs:comment>
    <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Need this for cataloging items for which we don’t use traditional call numbers, such as sound recordings and videos. No equivalent property found.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasPhysicalSubLocation">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has Physical Sub-Location</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an item and a physical sub-location for that item.</rdfs:comment>
    <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Equivalent BIBFRAME property has rdfs:domain = bf:Item, which is inaccurate in the RDA context.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasBarcode">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has Barcode</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an RDA Item and its barcode.</rdfs:comment>
    <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10003"/>
    <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Equivalent BIBFRAME property has rdfs:domain = bf:Barcode, which was judged too restrictive.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://doi.org/10.6069/uwlib.55.d.4#hasCreatorCharacteristic">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">Has Creator Characteristic</rdfs:label>
    <rdfs:comment xml:lang="en">A relationship between an RDA Work and a term identifying a characteristic of the Work’s creator.</rdfs:comment>
    <rdfs:domain rdf:resource="http://rdaregistry.info/Elements/c/C10001"/>
    <rdfs:isDefinedBy rdf:resource="https://doi.org/10.6069/uwlib.55.d.4"/>
    <skos:note xml:lang="en">Values for this property should resemble values for the MARC 386 field. The domain and range of the BIBFRAME property created for this purpose was judged too restrictive.</skos:note>
  </rdf:Description>
